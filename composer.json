{
    "name": "blanchonvincent/zendesk-remote-object",
    "description": "This library provide a REST client to ZenDesk remote call.",
    "authors": [
        {
            "name": "Blanchon Vincent",
            "email": "blanchon.vincent@gmail.com",
            "homepage": "http://github.com/blanchonvincent"
        }
    ],
    "require": {
        "fourmation/rest-remote-object": "1.0.0",
        "ocramius/proxy-manager": "0.5.1",
        "zendframework/zendframework": "2.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "~0.6",
        "fabpot/php-cs-fixer": "0.4.*@dev"
    },
    "autoload": {
        "psr-0": {
            "ZenDesk\\" : "src",
            "ZenDeskTest\\" : "tests",
            "ZenDeskTestAssets\\" : "tests"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7.0-dev"
        }
    }
}
